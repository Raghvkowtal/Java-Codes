package com.kodnest.alphabets;

public class Z {
	public static void main(String[] args) {
	int n=10;
	for(int i=0; i<=n; i++) {
		for(int j=0; j<=n; j++) {
			if(i==0||i==10||(i==1 && j==9)||(i==2 && j==8)) {
				System.out.print("*");
			}else
				System.out.print(" ");
		}
		System.out.println( );
	}
}
}



	
	
	
	
	
	
	
	
