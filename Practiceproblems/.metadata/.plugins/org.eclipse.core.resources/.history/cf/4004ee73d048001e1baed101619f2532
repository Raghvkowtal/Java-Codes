package com.kodnest.alphabets;

public class K {

	public static void main(String[] args) {
		for(int i=0; i<=10; i++) {
			for(int j=0; j<=10; j++) {
				if(j==0||(i==4 && j==2)
						||(i==5&&j==5)
						||(i==6&&j==5)||(i==7&&j==6)||(i==8&&j==7)||(i==9&&j==8)
						||(i==3 && j==6)||(i==2 && j==7)||(i==1 && j==8)) {
					System.out.print("*");
				}else
					System.out.print(" ");
			}
			System.out.println( );
		}
	}

}
