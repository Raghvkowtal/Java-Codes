package com.kodnest.alphabets;

public class K {

	public static void main(String[] args) {
		for(int i=0; i<=10; i++) {
			for(int j=0; j<=10; j++) {
				if(j==0||(i==5&&j==1)||(i==4&&j==2)||(i==3&&j==3)||(i==2&&j==4)||(i==1&&j==5)||(i==0&&j==6)) {
					System.out.print("*");
				}else
					System.out.print(" ");
			}
			System.out.println( );
		}
	}

}
