package com.kodnest.alphabets;

public class K {

	public static void main(String[] args) {
		for(int i=0; i<=10; i++) {
			for(int j=0; j<=10; j++) {
				if(i==0||i==10/2||i==10||j==0||i==j||j==i||j==10/2) {
					System.out.print("*");
				}else
					System.out.print(" ");
			}
			System.out.println( );
		}
	}

}
