package com.kodnest.complex11x11.pattern;

public class pattern1 {

	public static void main(String[] args) {
		for(int j=0; j<=11; j++) {
			if(j==0||j==10) {
				System.out.print("*");
			}
			System.out.println(" ");
			
		}
		for(int j=0; j<=10; j++) {
			if(j==0||j==10) {
				System.out.print("*");
			}
		}
	}

}
